{
  "name": "front-end",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:ssl": "ng serve --ssl",
    "config:to-base64": "node ./bin/to-base64.js",
    "config:from-base64": "node ./bin/from-base64.js",
    "lint": "ng lint --fix",
    "lint:scss": "stylelint \"./src/**/*.scss\" --fix",
    "lint:all": "npm run lint && npm run lint:scss",
    "prettier": "prettier --write \"src/**/*.*(ts|js|json|html)\"",
    "pretty-quick": "pretty-quick --staged --pattern \"src/**/*.*(ts|js|json|html)\"",
    "hook:pre-commit": "npm run pretty-quick && npm run lint:all"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/cdk": "^13.0.3",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/material": "^13.0.3",
    "@angular/material-moment-adapter": "^13.1.3",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "@aws-sdk/credential-providers": "^3.43.0",
    "@types/chart.js": "^2.9.35",
    "angularx-qrcode": "^13.0.2",
    "aws-amplify": "^4.3.10",
    "chart.js": "^3.7.0",
    "country-state-city": "^3.0.1",
    "hammerjs": "^2.0.8",
    "ng2-charts": "^3.0.8",
    "moment": "^2.29.1",
    "ng2-pdf-viewer": "~5.3.4",
    "pdfjs-dist": "~2.5.207",
    "ngx-image-cropper": "^5.0.1",
    "postcss": "^8.4.4",
    "postcss-scss": "^4.0.2",
    "rxjs": "~7.4.0",
    "swiper": "^7.4.1",
    "tslib": "^2.3.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.2",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.0.4",
    "@angular/compiler-cli": "~13.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@types/jasmine": "~3.10.0",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.transform": "^4.6.6",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "class-transformer": "^0.5.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "lodash.isequal": "^4.5.0",
    "lodash.transform": "^4.6.0",
    "ngx-infinite-scroll": "^10.0.1",
    "ngx-pagination": "^5.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.4",
    "postcss-scss": "^4.0.2",
    "prettier": "^2.5.0",
    "pretty-quick": "^3.1.2",
    "reset-css": "^5.0.1",
    "stylelint": "^14.2.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-declaration-strict-value": "^1.8.0",
    "stylelint-no-unsupported-browser-features": "^5.0.2",
    "stylelint-scss": "^4.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run hook:pre-commit"
    }
  }
}
